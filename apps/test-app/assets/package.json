    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "axios": "^0.18.1",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "^4.4.0",
    "css-loader": "2.1.1",
    "eslint": "6.4.0",

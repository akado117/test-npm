{
  "name": "courier_app",
  "description": "Delivery Platform for ScriptDrop",
  "repository": {},
  "scripts": {
    "courier-admin-testing": "node_modules/.bin/cypress run --spec 'cypress/test/integration/courier-admin/**'",
    "courier-app-tests": "node_modules/.bin/cypress run --spec 'cypress/test/e2e/courier/**','cypress/test/integration/courier/**'",
    "create": "./bin/create.sh",
    "cypress-test-runner": "node_modules/.bin/cypress open",
    "delete-source-maps": "rm -rf ../priv/static/js/*.js.map",
    "deploy": "webpack --mode production && npm run sentry && npm run delete-source-maps",
    "e2e": "cypress run --headed",
    "e2e-headed": "npm run ready-e2e && wait-on http://localhost:4002/js/app.js && cypress run --headed; result=$?; npm run teardown-e2e; exit $result",
    "e2e-local": "npm run ready-e2e && wait-on http://localhost:4002/js/app.js && cypress run; result=$?; npm run teardown-e2e; exit $result",
    "e2e-staging": "cypress_baseUrl=\"https://portal.staging.scriptdrop.co\" cypress run --headed",
    "flow": "flow",
    "format-directory": "node_modules/.bin/prettier-standard $1",
    "format-test": "npm run format-test:prettier-standard",
    "format-test:prettier-standard": "node_modules/.bin/prettier-standard \"./js/**/*.js\" \"./cypress/**/*.spec.js\" && npm run lint",
    "internal-dashboard-e2e": "node_modules/.bin/cypress run --spec 'cypress/test/e2e/internal-dashboard/**'",
    "internal-dashboard-int": "node_modules/.bin/cypress run --spec 'cypress/test/integration/internal-dashboard/**'",
    "lint": "./node_modules/eslint/bin/eslint.js \"./js/**/*.js\"",
    "pharmacy-portal-tests": "node_modules/.bin/cypress run --spec 'cypress/test/integration/pharmacy-portal/**','cypress/test/e2e/pharmacy-portal/**'",
    "ready-e2e": "npm run setup-app && npm run start-app",
    "sentry": "sentry-cli releases set-commits --auto $(sentry-cli releases propose-version) && sentry-cli releases finalize $(sentry-cli releases propose-version)",
    "setup-app": "../../../script/setup_e2e",
    "start-app": "npm run start-server && wait-on http://localhost:4002/js/app.js",
    "start-server": "cd ../ && MIX_ENV=e2e mix phx.server &",
    "start-server:local": "cd ../../.. && iex -S mix phx.server",
    "storybook": "start-storybook -s ./static -p 6006",
    "teardown-e2e": "kill -9 $(lsof -t -i :4002)",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose",
    "watch": "webpack --mode development --watch-stdin"
  },
  "dependencies": {
    "@babel/core": "7.7.2",
    "@babel/polyfill": "^7.4.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
